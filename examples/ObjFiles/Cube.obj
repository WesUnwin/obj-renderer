v  -0.5  0.5  0.5     # 1  Front, top left
v  -0.5 -0.5  0.5     # 2  Front, bottom left
v   0.5 -0.5  0.5     # 3  Front, bottom right
v   0.5  0.5  0.5     # 4  Front, top right

v  -0.5  0.5 -0.5     # 5  Back, top left
v  -0.5 -0.5 -0.5     # 6  Back, bottom left
v   0.5 -0.5 -0.5     # 7  Back, bottom right
v   0.5  0.5 -0.5     # 8  Back, top right


vt  0, 0  # top left
vt  0, 1  # bottom left
vt  1, 1  # bottom right
vt  1, 0  # top right


usemtl front
f  1 2 3   # Front
f  4 1 3

usemtl left
f  5 6 2   # Left side
f  5 2 1

usemtl right
f  4 3 7   # Right side
f  4 7 8

usemtl back
f  5/1 6/2 7/3   # Back side
f  8/4 5/1 7/3

usemtl top
f  5 1 4   # Top side
f  5 4 8

usemtl bottom
f  6 2 3   # Bottom side
f  6 3 7
